---

ssid: youraccesspointname
wpa_passphrase: yourwifipassword
domain_name: yourdomain

lan_interface_name: eth0
lan_interface_address: 10.0.0.254
lan_dhcp_range_start: 10.0.0.1
lan_dhcp_range_end: 10.0.0.50
lan_interface_netmask: 255.255.255.0

wifi_interface_name: wlan0
wifi_interface_address: 10.1.0.254
wifi_dhcp_range_start: 10.1.0.1
wifi_dhcp_range_end: 10.1.0.50
wifi_interface_netmask: 255.255.255.0

wan_interface_name: ppp0
wan_real_interface_name: eth1
wan_downbw_kbps: 6000  # 6 Mbps
wan_upbw_kbps: 800  # 0.8 Mbps
