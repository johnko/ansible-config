domain-needed
bogus-priv
local=/{{ domain_name }}/
interface={{ lan_interface_name }}
interface={{ wifi_interface_name }}
expand-hosts
domain={{ domain_name }}
dhcp-range={{ lan_interface_name }},{{ lan_dhcp_range_start }},{{ lan_dhcp_range_end }},{{ lan_interface_netmask }},12h
dhcp-range={{ wifi_interface_name }},{{ wifi_dhcp_range_start }},{{ wifi_dhcp_range_end }},{{ wifi_interface_netmask }},12h
dhcp-authoritative
no-negcache
